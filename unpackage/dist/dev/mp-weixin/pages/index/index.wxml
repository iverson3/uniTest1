<view class="content"><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background"></view><swiper class="carousel" circular data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['navToDetailPage',[['o',['id',item.id]]]]]]]}}" class="carousel-item" bindtap="__e"><image class="carousel-item-image" src="{{item.src}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num left-num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><uni-grid vue-id="1" column="{{4}}" data-event-opts="{{[['^change',[['clickGrid']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{cateList}}" wx:for-item="cate" wx:for-index="index" wx:key="index"><uni-grid-item vue-id="{{('2-'+index)+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><image class="image" src="{{cate.url}}" mode="aspectFill"></image><text class="text">{{cate.text}}</text></uni-grid-item></block></uni-grid><view class="navbar"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{['nav-item',(tabCurrentIndex===index)?'current':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></view><swiper class="swiper-box" current="{{tabCurrentIndex}}" duration="300" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{navList}}" wx:for-item="tabItem" wx:for-index="tabIndex" wx:key="tabIndex"><swiper-item class="tab-content"><block wx:if="{{tabItem.loaded===true&&tabItem.musicList.length===0}}"><empty vue-id="{{'3-'+tabIndex}}" bind:__l="__l"></empty></block><view class="uni-list"><block wx:for="{{tabItem.musicList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-newsid="{{item.id}}" data-event-opts="{{[['tap',[['gotoinfo',['$event']]]]]}}" bindtap="__e"><view class="uni-media-list"><image class="uni-media-list-logo" src="{{item.url}}"></image><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{item.name}}</view><view class="uni-media-list-text-bottom uni-ellipsis">{{item.created_at}}</view></view></view></view></block></view><uni-load-more vue-id="{{'4-'+tabIndex}}" status="{{tabItem.loadingType}}" bind:__l="__l"></uni-load-more></swiper-item></block></swiper><view class="uni-list"><view class="uni-list-cell"><view class="uni-media-list"><view class="uni-media-list-body"><block wx:if="{{hasLogin}}"><view>{{''+username+''}}<view data-event-opts="{{[['tap',[['tologout',['$event']]]]]}}" class="logout" bindtap="__e">退出登录</view></view></block><block wx:else><view><navigator url="../login/login" hover-class="navigator-hover"><button type="default">登录</button></navigator></view></block></view></view></view></view></view>